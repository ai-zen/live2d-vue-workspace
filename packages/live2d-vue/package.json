  },
  "devDependencies": {
    "@types/node": "^20.8.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.1",
    "vue-tsc": "^1.8.5"
