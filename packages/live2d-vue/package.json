{
  "name": "@ai-zen/live2d-vue",
  "version": "1.1.1",
  "description": "Live2D-Vue is a vue component lib for integrating Live2D views into Vue projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-zen/live2d-vue-workspace.git"
  },
  "bugs": {
    "url": "https://github.com/ai-zen/live2d-vue-workspace/issues"
  },
  "homepage": "https://github.com/ai-zen/live2d-vue-workspace",
  "type": "module",
  "main": "./dist/live2d-vue.umd.cjs",
  "module": "./dist/live2d-vue.js",
